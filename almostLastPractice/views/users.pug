extends layout

block content
  .container
    .row
      .col-10
        table.table.table-striped.table-hover
          thead
            tr
              th ID
              th Name
              th Skill
              th E-mail
          tbody
            each member in users
              tr 
                td #{user.id}
                td #{user.name}
                td #{user.skills}
                td #{user.email}